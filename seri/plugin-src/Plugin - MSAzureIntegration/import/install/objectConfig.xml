<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">

<sailpoint>
    <ImportAction name="merge">
        <ObjectConfig name="Bundle">
            <Attributes>
                <Map>
                    <entry key="roleTypeDefinitions">
                        <value>
                            <List>
                                <RoleTypeDefinition displayName="Launcher Business" icon="businessIcon" name="launcherBusiness" noDetection="true" noIIQ="true" noProfiles="true" notPermittable="true" notRequired="true">
                                    <Description>Defines a collection of users that perform similar functions within the organization.  Launcher Business Roles are associated with Launcher Roles or Entitlement Roles to provide specific levels of access to enterprise IT resources.</Description>
                                    <RequiredRights>
                                        <Reference class="sailpoint.object.SPRight" name="ManageLauncherBusinessRoles"/>
                                    </RequiredRights>
                                </RoleTypeDefinition>
                                <RoleTypeDefinition displayName="Launcher" icon="itIcon" name="launcher" noAssignmentSelector="true" noIIQ="true" noPermits="true" noRequirements="true">
                                    <Description>A SSO Launcher Role</Description>
                                    <RequiredRights>
                                        <Reference class="sailpoint.object.SPRight" name="ManageLauncherRoles"/>
                                    </RequiredRights>
                                </RoleTypeDefinition>
                            </List>
                        </value>
                    </entry>
                </Map>
            </Attributes>
            <ObjectAttribute displayName="Azure Dynamic Query" name="dynamicQuery"/>
        </ObjectConfig>
    </ImportAction>
    <ImportAction name="merge">
        <ObjectConfig name="ManagedAttribute">
            <ObjectAttribute displayName="Azure Dynamic Query" name="dynamicQuery"/>
        </ObjectConfig>

    </ImportAction>
</sailpoint>

