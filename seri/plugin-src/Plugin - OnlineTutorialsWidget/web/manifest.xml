<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE Plugin PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Plugin displayName="Online Tutorials" minSystemVersion="7.1" name="onlineTutorials" rightRequired="onlineTutorialsWidget" version="1.0">
  <Attributes>
    <Map>
      <entry key="minUpgradableVersion" value="0.1"/>
        <entry key="snippets">
        <value>
          <List>
            <Snippet regexPattern=".*\/home\.?(jsf|xhtml)" rightRequired="onlineTutorialsWidget">
              <Scripts>
                <String>ui/js/AbstractListCtrl.js</String>
                <String>ui/js/OnlineTutorials.js</String>
                <String>ui/js/OnlineTutorialsService.js</String>
                <String>ui/js/OnlineTutorialsWidgetDirective.js</String>
                <String>ui/js/OnlineTutorialsWidgetDirectiveCtrl.js</String>
              </Scripts>
                  <StyleSheets>
                <String>ui/css/seri-widget.css</String>
              </StyleSheets>
            </Snippet>
          </List>
        </value>
      </entry>
      <entry key="restResources">
        <value>
          <List>
            <String>sailpoint.plugin.onlinetutorials.rest.OnlineTutorialsResource</String>
          </List>
        </value>
      </entry>
    </Map>
  </Attributes>
</Plugin>
