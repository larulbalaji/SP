<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE TaskDefinition PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<TaskDefinition name="Identity Refresh - Remove Joiner Event Health Care" resultAction="Delete" subType="task_item_type_identity" type="Identity">
  <Attributes>
    <Map>
      <entry key="promoteAttributes" value="true"/>
      <entry key="refreshManagerStatus" value="true"/>
      <entry key="refreshRule" value="Identity Refresh - Clear needsCreateProcessing Trigger"/>
      <entry key="filter" value="(department == &quot;Nursing&quot; || department == &quot;Doctors&quot;)"/>
    </Map>
  </Attributes>
  <Description>Runs a rule on each identity to stop processing of the
    Joiner Event (for initial loading)</Description>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <Parent>
    <Reference class="sailpoint.object.TaskDefinition" name="Identity Refresh"/>
  </Parent>
</TaskDefinition>
