<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE AlertDefinition PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<AlertDefinition displayName="Last Login Alert" name="Last Login Alert">
  <ActionConfig actionType="NOTIFICATION">
    <Attributes>
      <Map>
        <entry key="emailTemplate" value="Alert Notification"/>
        <entry key="identityEmailRecipient">
          <value>
            <List>
              <String>spadmin</String>
            </List>
          </value>
        </entry>
      </Map>
    </Attributes>
  </ActionConfig>
  <Description>sends notification</Description>
  <AlertMatchConfig>
    <MatchRule>
      <Reference class="sailpoint.object.Rule" name="AlertMatcherRule"/>
    </MatchRule>
  </AlertMatchConfig>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
</AlertDefinition>