<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE sailpoint PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<sailpoint>
<Identity name="BlockList" workgroup="true">
  <Attributes>
    <Map>
      <entry key="displayName" value="BlockList"/>
    </Map>
  </Attributes>
  <Description>List of users who should not receive notifications, escalations, or work items.</Description>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <Preferences>
    <Map>
      <entry key="workgroupNotificationOption">
        <value>
          <WorkgroupNotifationOption>Disabled</WorkgroupNotifationOption>
        </value>
      </entry>
    </Map>
  </Preferences>
</Identity>
</sailpoint>
