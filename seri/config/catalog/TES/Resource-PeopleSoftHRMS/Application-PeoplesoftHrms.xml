      <entry key="getObjectSQL" value="SELECT Person.EMPLID,Person.NAME_PREFIX,Person.NAME_SUFFIX,Person.FIRST_NAME,Person.LAST_NAME, Person.MIDDLE_NAME,Person.NAME,Person.NAME_TITLE,Person.ADDRESS1,Person.ADDRESS2,Person.ADDRESS3, Person.BIRTHDATE, Person.CITY,Person.COUNTRY,Person.PREF_FIRST_NAME,Person.POSTAL, Person.HR_RESPONSIBLE_ID,Person.STATE,Email.E_ADDR_TYPE,Email.EMAIL_ADDR,Phone.PHONE_TYPE,Phone.PHONE FROM PS_PERSONAL_DATA Person LEFT OUTER JOIN (SELECT tempPerson.EMPLID,tempEmail.E_ADDR_TYPE,tempEmail.EMAIL_ADDR FROM PS_PERSONAL_DATA tempPerson LEFT OUTER JOIN PS_EMAIL_ADDRESSES tempEmail ON tempPerson.EMPLID = tempEmail.EMPLID WHERE tempEmail.E_ADDR_TYPE='BUSN') Email ON Person.EMPLID = Email.EMPLID LEFT OUTER JOIN (SELECT tempPerson.EMPLID,tempPhone.PHONE,tempPhone.PHONE_TYPE FROM PS_PERSONAL_DATA tempPerson LEFT OUTER JOIN PS_PERSONAL_PHONE tempPhone ON tempPerson.EMPLID = tempPhone.EMPLID WHERE tempPhone.PHONE_TYPE='BUSN') Phone ON Person.EMPLID = Phone.EMPLID AND Email.EMPLID = Phone.EMPLID WHERE Person.EMPLID = '$(identity)'"/>
      <entry key="getSubSQL" value="SELECT  Job.EMPL_RCD,Job.PER_ORG,Job.DEPTID,Job.JOBCODE,Job.POSITION_NBR,Job.SUPERVISOR_ID,Job.HR_STATUS,Job.EMPL_STATUS, Job.ACTION,Job.ACTION_REASON,Job.LOCATION,Job.FULL_PART_TIME,Job.COMPANY,Job.EMPL_TYPE,Job.OFFICER_CD,Job.EMPL_CLASS,Job.ACCT_CD, Job.BUSINESS_UNIT,Job.REPORTS_TO,Job.TERMINATION_DT,Job.HIRE_DT, Job.JOB_INDICATOR,Job.EMPLID,Job.EFFDT FROM PS_JOB Job WHERE Job.EMPLID = '$(identity)' and Job.JOB_INDICATOR = 'P' AND Job.EFFDT = (SELECT MAX(Job1.EFFDT) FROM PS_JOB Job1 WHERE Job1.EMPLID = Job.EMPLID AND Job1.EMPL_RCD = Job.EMPL_RCD AND Job1.EFFDT &lt;= CURRENT_TIMESTAMP)  AND Job.EFFSEQ = (SELECT MAX(Job2.EFFSEQ) FROM PS_JOB Job2 WHERE Job2.EMPLID = Job.EMPLID AND Job2.EMPL_RCD = Job.EMPL_RCD AND Job2.EFFDT = Job.EFFDT)"/>
