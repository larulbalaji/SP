	</tr>
	<tr height="20px">
		<td class="spacer-left"></td>
		<td class="spacer-right"></td>
	</tr>
	<tr height="100%">
		<td class="main-left"></td>
		<td class="main-right">
<table><tr><td  class="list">
  
  <h2>Version: $readme.version</h2>
#foreach ( $section in $readme.sections )
