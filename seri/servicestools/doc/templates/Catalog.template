<html>
	<head>
		<link href="css/seri.css" rel="stylesheet" type="text/css" />
	</head>
	
	<body>
<div id="page">
<table style="border-spacing: 0px; border: 1px;" width="100%">
	<tr height="100px">
		<td width="100px" class="header-left"/>
		<td width="100%" class="header-right">
			<div style="float:left">
				<h1>SERI Catalog</h1>
			</div>
			<div style="float:right; margin-top: 10px; margin-right: 10px;"><img src="images/sailpoint-wht-sml.png"/></div>
		</td>
	</tr>
	<tr height="20px">
		<td class="spacer-left"></td>
		<td class="spacer-right"></td>
	</tr>
	<tr height="100%">
		<td class="main-left"></td>
		<td class="main-right">
<table><tr><td  class="list">
#foreach ( $section in $catalog )
<h2>$section.sectionName</h2>
#foreach ( $entry in $section.entries )
#if ( $entry.valid ) <a href="$section.sectionName/${entry.name}-readme.html">$entry.name</a><br/>
#else $entry.name<br/>
#end
#end
#end
</td></tr></table>
		</td>
	</tr>
</table>
</div>
	</body>
</html>
