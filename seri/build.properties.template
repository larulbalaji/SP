IIQVersion=7.1
build.properties.version=15
########################
#  IIQ Base (Required) #
########################
#
# **Either** location of GA zip file identityiq-version.zip
#
GALocation=S:/SailPoint/IIQ7.1 GA
#
# **Or** location of identityiq.war 
#
#WARLocation=D:/workspace-juno/identityiq/trunk/image

################################################
#  IIQ Patch (only required if applying patch) #
################################################
#IIQPatchLevel=p4
#PatchLocation=C:/sailpoint/patches

runCustomScripts=false

#dev properties
#make sure any app server specific env variables,like CATALINA_HOME for tomcat, are set on your system
IIQHome=D:/iiq-testing/7.0/tomcat/webapps/identityiq
application.server.host=localhost
application.server.port=8080

#####################################################################################
# This one is only needed when we're using the 'importDemoData' target              #
# Typically when the demodata project has changed and we need to re-import the data #
# Our rendition of demodata will be checked into the SERI project                   #
#####################################################################################
demodata.location=G:/Documents/SailPoint/demodata

# These are required to start/stop tomcat from the ant script (ant tomcat.start and ant tomcat.stop)
application.server.home=D:/iiq-testing/7.0/tomcat
application.server.start=D:/iiq-testing/7.0/tomcat/bin/startup.bat
application.server.stop=D:/iiq-testing/7.0/tomcat/bin/shutdown.bat
db.url=jdbc:mysql://localhost/identityiq?useServerPrepStmts=true&tinyInt1isBit=true&useUnicode=true&characterEncoding=utf8
db.userid=root
db.password=password
#type must be db2,mysql,oracle,sqlserver
db.type=mysql
#set this to true if you make test scripts that call targets with user warnings like drop db etc.
override.safety.prompts=false
#for export, the original install date string that we can use to determine new or changed objects
installDate=4/25/12 14:48:58 PM CDT

#tomcat properties. You only need to set these if you have a CATALINA_HOME env var set
#NOTE: for tomcat 7+ the manager url is /manager/text for 6 and lower its just /manager
#You will also need to setup a manager-script user in your tomcat user config, check out
#http://tomcat.apache.org/tomcat-7.0-doc/manager-howto.html#Executing_Manager_Commands_With_Ant
manager.url=http://localhost:8080/manager/text
manager.username=tomcat
manager.password=tomcat

###############################################################
# Custom branding                                             #
# This parameter will replace login.gif and headerSPLogo.jpg  #
# with login-xxx.gif and headerSPLogo-xxx.jpg from web/images #
# For 7.x branding use following images in web/images         #
# loginLogo-xxx.png, TopLogo1-xxx.png, watermark-xxx.png      #
# mobilelogo-xxx.png                                          #
# Desktop css: web/css/iiq-custom-xxx.css                     #
# Mobile css:  web/ui/css/ui-custom-xxx.css                   #
###############################################################
#seri.branding=xxx

# setting the snapshot date with "ant set-snapshot" allows exporting of only artifacts changed after that
# date with "ant export-snapshot". XML files will be placed in snapshot.location
snapshot.date=
snapshot.location=D:/temp/snapshot

####################
# Since version 15 #
####################
# Adding attachments to the Access Requests 
# see default.target.properties file for further configuration options.
# see UseCase-LCMAttachments/readme.txt for further documentation
#withLCMAttachments

####################
# Since version 14 #
####################
# For mysql formerly no database name had to be supplied, from now on all db types are treated equally
# a database name has to be added to the url. 'identityiq' (all lower case) should be used in the db url
# This will then be substituted for instance.name during the build if appropriate



